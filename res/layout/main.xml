<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/screen" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ViewStub android:id="@+id/mapview_stub" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/header_ad" android:layout_alignParentTop="true"
		android:layout_width="fill_parent" android:gravity="center"
		android:visibility="gone">
		<jp.co.nobot.libYieldMaker.libYieldMaker
			android:id="@+id/admakerview" android:layout_width="fill_parent"
			android:layout_weight="0" android:layout_height="70px" />
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/header" android:layout_alignParentTop="true"
		android:layout_width="fill_parent" android:gravity="right|center"
		android:background="#99000000" android:padding="5px"
		android:visibility="gone">
		<LinearLayout android:id="@+id/linearLayout1" android:layout_height="fill_parent"
			android:orientation="horizontal" android:layout_width="fill_parent" android:layout_weight="1">
			<LinearLayout android:layout_marginRight="2dip"
				android:layout_width="wrap_content" android:id="@+id/LinearLayout19"
				android:gravity="center" android:orientation="vertical" android:layout_height="fill_parent">
				<TextView android:shadowDx="1.2" android:textColor="#FFFFFF"
					android:shadowRadius="1.2" android:shadowDy="1.2"
					android:layout_height="wrap_content" android:id="@+id/TextView01"
					android:layout_width="wrap_content" android:shadowColor="#000000"
					android:textSize="14dip" android:text="会員"></TextView>
				<TextView android:shadowDx="1.2" android:textColor="#FFFFFF"
					android:shadowRadius="1.2" android:shadowDy="1.2"
					android:layout_height="wrap_content" android:id="@+id/trace_mylocation_label"
					android:layout_width="wrap_content" android:shadowColor="#000000"
					android:textSize="14dip" android:text="価格"
					android:layout_gravity="center"></TextView>
			</LinearLayout>
			<ToggleButton android:text="@+id/ToggleButton01"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:id="@+id/toggle_member"></ToggleButton>
		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout1" android:layout_height="fill_parent"
			android:orientation="horizontal" android:layout_width="fill_parent" android:layout_weight="1">
			<LinearLayout android:layout_marginRight="2dip"
				android:layout_width="wrap_content" android:id="@+id/LinearLayout19"
				android:gravity="center" android:orientation="vertical" android:layout_height="fill_parent">
				<TextView android:shadowDx="1.2" android:textColor="#FFFFFF"
					android:shadowRadius="1.2" android:shadowDy="1.2"
					android:layout_height="wrap_content" android:id="@+id/TextView01"
					android:layout_width="wrap_content" android:shadowColor="#000000"
					android:textSize="14dip" android:text="24H"></TextView>
				<TextView android:shadowDx="1.2" android:textColor="#FFFFFF"
					android:shadowRadius="1.2" android:shadowDy="1.2"
					android:layout_height="wrap_content" android:id="@+id/trace_mylocation_label"
					android:layout_width="wrap_content" android:shadowColor="#000000"
					android:textSize="14dip" android:text="ONLY"
					android:layout_gravity="center"></TextView>
			</LinearLayout>
			<ToggleButton android:text="@+id/ToggleButton01"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:id="@+id/toggle_rtc"></ToggleButton>
		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout2" android:layout_height="fill_parent"
			android:orientation="horizontal" android:layout_width="fill_parent" android:layout_weight="1">
			<LinearLayout android:layout_marginRight="2dip"
				android:layout_width="wrap_content" android:id="@+id/LinearLayout05"
				android:gravity="center" android:orientation="vertical" android:layout_height="fill_parent">
				<TextView android:shadowDx="1.2" android:textColor="#FFFFFF"
					android:shadowRadius="1.2" android:shadowDy="1.2"
					android:layout_height="wrap_content" android:id="@+id/TextView01"
					android:layout_width="wrap_content" android:shadowColor="#000000"
					android:textSize="14dip" android:text="現在地"></TextView>
				<TextView android:shadowDx="1.2" android:textColor="#FFFFFF"
					android:shadowRadius="1.2" android:shadowDy="1.2"
					android:layout_height="wrap_content" android:id="@+id/trace_mylocation_label"
					android:layout_width="wrap_content" android:shadowColor="#000000"
					android:textSize="14dip" android:text="追跡" android:layout_gravity="center"></TextView>
			</LinearLayout>
			<ToggleButton android:text="@+id/ToggleButton01"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:id="@+id/trace_mylocation"></ToggleButton>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_height="wrap_content" android:layout_alignParentBottom="true"
		android:layout_gravity="center" android:layout_width="fill_parent"
		android:id="@+id/footer" android:padding="5px" android:background="#99000000">
		<LinearLayout android:id="@+id/LinearLayout01"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:gravity="center_horizontal|center" android:layout_width="wrap_content">
			<ImageView android:id="@+id/search_img"
				android:layout_marginRight="1px" android:layout_marginLeft="1px"
				android:src="@drawable/search" android:layout_height="fill_parent"
				android:layout_width="fill_parent" android:scaleType="center"></ImageView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout02"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:gravity="center_vertical|center" android:layout_width="wrap_content">
			<ImageView android:layout_height="wrap_content" android:id="@+id/main_list"
				android:visibility="invisible" android:layout_marginLeft="1px"
				android:layout_marginRight="1px" android:src="@drawable/list"
				android:scaleType="center" android:layout_width="fill_parent"></ImageView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout03"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:gravity="center_vertical|center" android:layout_width="wrap_content">
			<ImageView android:layout_marginLeft="1px"
				android:layout_marginRight="1px" android:id="@+id/favorite"
				android:layout_width="fill_parent" android:scaleType="center"
				android:layout_height="fill_parent" android:src="@drawable/favorite"></ImageView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout04"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:layout_gravity="center_vertical" android:gravity="center"
			android:layout_width="wrap_content">
			<ImageView android:layout_height="wrap_content"
				android:layout_gravity="center" android:layout_marginLeft="1px"
				android:layout_marginRight="1px" android:id="@+id/zoomout"
				android:src="@drawable/zoomout" android:layout_width="fill_parent"
				android:scaleType="center"></ImageView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout05"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:gravity="center" android:layout_width="wrap_content">
			<ImageView android:layout_height="wrap_content"
				android:layout_marginLeft="1px" android:layout_marginRight="1px"
				android:id="@+id/zoomin" android:src="@drawable/zoomin"
				android:layout_width="fill_parent" android:scaleType="center"></ImageView>
		</LinearLayout>



	</LinearLayout>



</RelativeLayout>
